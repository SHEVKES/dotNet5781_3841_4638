        <Label Content="הקש מרחק" HorizontalAlignment="Left" Margin="303,128,0,0" VerticalAlignment="Top"/>
        <Label Content="הקש זמן" HorizontalAlignment="Left" Margin="312,184,0,0" VerticalAlignment="Top"/>
        <Label Content="ערוך מרחק וזמן בין תחנות" HorizontalAlignment="Left" Margin="76,48,0,0" VerticalAlignment="Top" Height="49" Width="253"/>
        <Button Content="שמור שינויים" HorizontalAlignment="Left" Margin="250,329,0,0" VerticalAlignment="Top" Width="129" Height="35" Click="Button_Click_Save"/>
        <Button Content="ביטול" HorizontalAlignment="Left" Margin="45,328,0,0" VerticalAlignment="Top" Width="131" Height="37" Click="Button_Click_Cancel"/>

    </Grid>
</Window>
